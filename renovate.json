{
  "extends": [
    "config:base",
    ":semanticCommits"
  ],
  "packageRules": [
    {
      "packageNames": ["typescript"],
      "groupName": ["typescript"]
    },
    {
      "packagePatterns": ["^react|^webpack|^rollup|^style"],
      "groupName": ["web dependencies"]
    },
    {
      "packagePatterns": ["*"],
      "groupName": "misc dependencies",
      "groupSlug": "misc"
    }
  ],
  "postUpdateOptions": ["yarnDedupeHighest"]
}
