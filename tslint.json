{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": false,
    "max-classes-per-file": false,
    "ordered-imports": true,
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "no-namespace": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "always"
        },
        "esSpecCompliant": true
      }
    ]
  }
}
